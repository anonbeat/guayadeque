        <item replace=" _@,;&amp;\/'&quot;." with="-"/>
      </replace>
      <extract>
        <item tag="&lt;div class=&quot;tab_original&quot;&gt;"/>
      </extract>
    </lyricsource>
    <lyricsource type="download" enabled="true" name="vagalume.uol.com.br (Portuguese translations)" source="http://vagalume.uol.com.br/{al}/{tl}-traducao.html">
      <replace>
        <item replace=" _@,;&amp;\/'&quot;." with="-"/>
      </replace>
      <extract>
        <item tag="&lt;div class=&quot;tab_tra_pt&quot;&gt;"/>
      </extract>
    </lyricsource>
  </lyricsources>
  <lyrictargets>
    <lyrictarget type="embedded" enabled="true" name="Embedded" source=""/>
    <lyrictarget type="file" enabled="false" name="To file" source="~/.guayadeque/lyrics/{a}/{t}.lyric">
      <replace>
        <item replace=" &lt;&gt;:&quot;\/|?*" with="_"/>
      </replace>
    </lyrictarget>
    <lyrictarget type="command" enabled="false" name="Command" source=""/>
  </lyrictargets>
</lyricengine>
