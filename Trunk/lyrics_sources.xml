        <item replace="." with=""/>
      </replace>
      <extract>
        <item tag="&lt;div id=&quot;songlyrics&quot; &gt;"/>
        <item tag="&lt;p&gt;"/>
      </extract>
    </lyricsource>
    <lyricsource type="download" enabled="true" name="lyriki.com" source="http://www.lyriki.com/{al}:{tl}">
