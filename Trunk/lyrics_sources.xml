        <item begin="[ " end=" ]"/>
      </exclude>
      <notfound>
        <item tag="Sorry, we have no"/>
        <item tag="This is an upcoming album and we do not have the"/>
      </notfound>
    </lyricsource>
    <lyricsource type="download" enabled="true" name="vagalume.uol.com.br" source="http://vagalume.uol.com.br/{al}/{tl}.html">
